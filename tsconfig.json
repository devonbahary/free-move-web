{
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@bodies/*": ["src/free-move/bodies/*"],
            "@collisions/*": ["src/free-move/collisions/*"],
            "@collision-events/*": ["src/free-move/collision-events/*"],
            "@utils/*": ["src/free-move/utils/*"],
            "@vectors/*": ["src/free-move/vectors/*"]
        }
    },
    "include": ["./src/**/*", "babel.config.js"]
}
